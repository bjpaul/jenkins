pipeline{
    agent{
        docker{
            image 'gcr.io/kaniko-project/executor:debug'
            args  '--entrypoint sleep'
        }
    }
    stages{
        stage("Init"){
            steps{
                sh "30s"
            }
        }
        stage("Next"){
            steps{
                sh "cat /kaniko/executor"
            }
        }
    }
}
